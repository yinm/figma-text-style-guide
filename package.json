{
  "name": "figma-text-style-guide",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "pnpm build:plugin & pnpm build:ui",
    "build:plugin": "vite build -c ./vite.config.plugin.ts",
    "build:ui": "vite build -c ./vite.config.ui.ts",
    "lint": "eslint 'src/'",
    "lint:fix": "eslint --fix 'src/'",
    "format": "prettier --check 'src/'",
    "format:fix": "prettier --write 'src/'"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.96.0",
    "@types/node": "^20.14.8",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^9.5.0",
    "eslint-config-yinm": "^0.0.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "prettier": "^3.3.2",
    "typescript": "^5.2.2",
    "vite": "^5.3.1",
    "vite-plugin-singlefile": "^2.0.2"
  }
}
